<h1>My Profile</h1>

<p>
    Number of cards: <%= cards.length %>
</p>

<table border="1">
    <tr>
        <th>question</th>
        <th>answer</th>
    </tr>
    <% cards.forEach(card => { %>
        <tr>
            <td>
                <%= card.question %>
            </td>
            <td>
                <%= card.answer %>
            </td>
        </tr>
    <% }); %>
</table>
